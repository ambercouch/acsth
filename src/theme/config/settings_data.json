          "fd275c91-3cce-4945-8d5a-39b5151f5c6e": {
            "type": "question",
            "settings": {
              "content_question": "Can I cancel my order?",
              "content_answer": "<p>You can't cancel your order if you have already received your order confirmation. If you want to cancel your order you can return it within 21 days from when it was received and we will refund the products value.<\/p>"
            }
          },
          "01c01de5-36d3-4c54-9a54-b99eae569bf6": {
