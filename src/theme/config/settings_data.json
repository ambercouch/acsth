          "7ba2c3d9-5737-43b8-9335-2216aa185ff4": {
            "type": "image_content",
            "settings": {
              "content_title": "RHYS GEORGE",
              "content_role": "CEO",
              "content_body": "<p>FMCG Commercial Background. Mentor to multiple businesses and founder of Visionary Group and Visionary Food Solutions.<br\/>Leading the Bearhug pack since its inception in 2016.<br\/><em>Believes he has a 100mph squash serve.<\/em><\/p>",
              "content_image": "shopify:\/\/shop_images\/ambassador-rhys.jpg",
              "block_background_colour": "t",
              "block_direction": "",
              "block_layout": "has-layout-2575",
              "content_btn_1_label": "Linkedin",
              "content_btn_1_link": "https:\/\/www.linkedin.com\/in\/startupandscaleupceo\/",
              "content_btn_2_label": "Find Out More",
